{
  "name": "react-starter",
  "version": "0.0.0",
  "description": "My starting point for React apps",
  "main": "dist/app.js",
  "scripts": {
    "build": "webpack",
    "deploy": "npm run build && gh-pages -d dist",
    "doc": "jsdoc -c .jsdoc src",
    "doc:live": "concurrently --kill-others \"npm run doc:watch\" \"npm run doc:view\"",
    "doc:view": "live-server dist/doc --port=9124",
    "doc:watch": "nodemon --exec \"npm run doc\" --watch src --watch ./ --ext js,md --ignore dist",
    "postversion": "git push && git push --tags && npm run deploy",
    "precommit": "pretty-quick --staged && npm test && npm run lint",
    "prepush": "npm test && npm run lint",
    "prettier": "prettier '{src,test}/**/*.js' --write",
    "preversion": "npm test",
    "start": "webpack-dev-server --config webpack.test.config.js",
    "lint": "eslint src test webpack.*",
    "test": "mocha test",
    "test:browsers": "karma start",
    "test:watch": "nodemon --exec \"npm test\" --watch src --watch test"
  },
  "author": "Jeremy Kahn <jeremyckahn@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@jeremyckahn/minami": "^1.3.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.2.0",
    "eslint-plugin-react": "^7.10.0",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.12.0",
    "jsdom-global": "^3.0.2",
    "karma": "^2.0.5",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ievms": "git+https://github.com/jeremyckahn/karma-ievms.git#browser-decorator",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^3.0.0",
    "live-server": "^1.2.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.0",
    "pretty-quick": "^1.6.0",
    "raf": "^3.4.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "compass-mixins": "^0.12.10",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
}
